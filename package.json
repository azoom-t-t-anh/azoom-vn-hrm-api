{
  "name": "azoom-hr-management",
  "version": "1.0.6",
  "description": "An application to receive and process image (crop, compress, resize)",
  "author": "Azoom",
  "license": "The Unlicense",
  "private": true,
  "scripts": {
    "build:doc": "yarn docker:build:specs:image && yarn build:doc:docker",
    "dev": "cd app && yarn dev",
    "specs": "yarn specs:build && redoc-cli serve ./app/openapi.json",
    "specs:build": "cd specs && multi-file-swagger openapi.yaml > ../app/openapi.json"
  },
  "dependencies": {
    "multi-file-swagger": "^2.3.0",
    "redoc-cli": "^0.9.7"
  }
}
